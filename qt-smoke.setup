;; -*- scheme -*-

(compile qtcore.scm -j qtcore -s -O2 -d0 -c++
         -lsmokeqtcore)
(compile qtcore.import.scm -s -O2 -d0 -c++
         -lsmokeqtcore)

(compile qtgui.scm -j qtgui -s -O2 -d0 -c++
         -lsmokeqtgui)
(compile qtgui.import.scm -s -O2 -d0 -c++
         -lsmokeqtgui)

(install-extension
 'qt-smoke
 '("qtcore.so" "qtcore.import.so"
   "qtgui.so" "qtgui.import.so")
 '((version 0.1)))
